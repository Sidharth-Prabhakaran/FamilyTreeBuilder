<html>
<head>
  <title>Your Trees</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 20px;
      background-color: #f5f5f5;
    }

    .navbar {
      background-color: #333;
      padding: 10px;
      color: #fff;
    }

    .navbar ul {
      list-style-type: none;
      margin: 0;
      padding: 0;
      display: flex;
      justify-content: center;
    }

    .navbar li {
      margin-right: 10px;
    }

    .navbar li a {
      display: inline-block;
      padding: 10px 20px;
      color: #fff;
      text-decoration: none;
    }

    .navbar li a.active {
      background-color: #ccc;
      border-radius: 5px;
    }

    .logout-form {
      display: inline-block;
    }
  </style>
</head>
<body>
  <div class="navbar">
    <ul>
      <li><a href="/">Home</a></li>
      <li><a href="/profile">Your Trees</a></li>
        <li><a href="/createTree">Create Tree</a></li>
      <li>
        <form class="logout-form" action="/logout?_method=DELETE" method="post">
          <button type="submit">Logout</button>
        </form>
      </li>
    </ul>
  </div>
  <h1>Below are The Trees with their appropriate access</h1>
  <ul>
    <% for (let tree of coaches) { %>
      <li>
        <%= tree.tree_name %>
        <% if (tree.access_level === 'view') { %>
          <a href="/tree/<%= tree.id %>/view">View</a>
        <% } else if (tree.access_level === 'edit') { %>
          <a href="/tree/<%= tree.id %>/edit">Edit</a>
          <a href="/tree/<%= tree.id %>/delete">Delete</a>
        <% } %>
      </li>
    <% } %>
  </ul>

 
</body>
</html>



